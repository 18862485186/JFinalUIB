	<build>
		<finalName>UIB</finalName>
		
		<resources>
			<resource>
				<directory>src/main/java</directory>
				<includes>
                    <include>**/*.xml</include>
                </includes>
                <filtering>true</filtering>
			</resource>
			
			<resource>
				<directory>src/main/resources</directory>
				<includes>
					<include>*.properties</include>
					<include>*.xml</include>
				</includes>
                <filtering>true</filtering>
			</resource>
		</resources>
		
		<testResources>
