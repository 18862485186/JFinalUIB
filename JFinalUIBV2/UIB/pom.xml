				<includes>
                    <include>**/*.xml</include>
                </includes>
                <filtering>true</filtering>
			</testResource>
			
			<testResource>
				<directory>src/main/resources</directory>
				<includes>
                    <include>**/*.xml</include>
					<include>**/*.properties</include>
                </includes>
				<filtering>true</filtering>
			</testResource>
		</testResources>
		
		<plugins>
            <!-- Compile -->
